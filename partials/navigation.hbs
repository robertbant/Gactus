<nav class="main-nav">
    {{#foreach navigation}}
    <a href="{{url absolute="true"}}">{{label}}</a>
    {{/foreach}}

    {{#if @labs.subscribers}}
    <a class="cta" href="{{@blog.url}}/subscribe/">Subscribe</a>
    {{else}}
    <a class="cta" href="{{@blog.url}}/rss/">Subscribe</a>
    {{/if}}

    {{#if @blog.twitter}}
    <a class="menu-border" href="{{twitter_url @blog.twitter}}" target="_blank">Twitter</a>
    {{/if}}
</nav>
